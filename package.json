{
  "name": "aoetw",
  "version": "0.1.0",
  "description": "完整、準確的世紀帝國百科全書",
  "author": "leeci",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "dev-debug": "node --max-old-space-size=4096 --inspect node_modules/nuxt/bin/nuxt",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "node --max_old_space_size=4096 ./node_modules/.bin/nuxt generate",
    "format-code": "prettier --write \"pages/**/*.js\" \"pages/**/*.vue\""
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc",
    "clipboard": "^2.0.4",
    "codemirror": "^5.43.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "lodash": ">=4.17.13",
    "nuxt": "^2.15.3"
  },
  "devDependencies": {
    "@babel/standalone": "^7.3.1",
    "@fortawesome/fontawesome-free": "^5.7.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "2.6.2",
    "ftp-deploy": "^2.3.0",
    "nodemon": "^1.11.0",
    "prettier": "1.16.1"
  }
}
